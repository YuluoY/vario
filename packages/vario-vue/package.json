{
  "name": "@variojs/vue",
  "version": "0.1.3",
  "description": "Vario Vue Renderer - Vue 3 integration for Vario Schema rendering",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "node ../../scripts/minify.js",
    "dev": "tsc --watch",
    "test": "NODE_OPTIONS=--max-old-space-size=4096 vitest run",
    "test:perf": "NODE_OPTIONS=--max-old-space-size=8192 vitest run __tests__/performance.test.ts",
    "test:comprehensive": "NODE_OPTIONS=--max-old-space-size=4096 vitest run __tests__/model-path-comprehensive.test.ts",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "vitest": "^1.2.0",
    "vue": "^3.4.0",
    "element-plus": "^2.4.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@variojs/core": "workspace:*",
    "@variojs/schema": "workspace:*"
  }
}